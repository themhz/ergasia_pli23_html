<!ELEMENT doctorData (doctor,center,apointments)>

<!ELEMENT doctor (fullname, artaftotita, email)>

<!ELEMENT center (address,tk,phone)>
<!ELEMENT fullname (#PCDATA)>
<!ELEMENT artaftotita (#PCDATA)>
<!ELEMENT email (#PCDATA)>
